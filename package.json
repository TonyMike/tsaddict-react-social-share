{
  "name": "@tsaddict/react-social-share",
  "version": "0.1.4",
  "description": "Lightweight React social share buttons with Tailwind animations and React Icons.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": ">=4.0.0",
    "tailwindcss": ">=3.0.0 || ^4.0.0-alpha"
  },
  "keywords": [
    "react",
    "share",
    "social",
    "tailwind",
    "react-icons"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.1.12",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
